{
    "name": "mattmangoni/nova-blogify-tool",
    "description": "Create a simple blogging system inside Laravel Nova in one click",
    "keywords": [
        "laravel",
        "nova",
        "blogging",
        "generator"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "dimsav/laravel-translatable": "^9.2",
        "intervention/image": "^2.4.0",
        "jameslkingsley/nova-media-library-field": "^1.0",
        "spatie/laravel-medialibrary": "^7.5",
        "itsimplycom/nova-translatable": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Mattmangoni\\NovaBlogifyTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mattmangoni\\NovaBlogifyTool\\BlogifyToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
